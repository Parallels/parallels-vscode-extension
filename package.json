                {
                    "command": "parallels-desktop.vagrant.search-and-download",
                    "when": "parallels-desktop:vagrant == true"
                }
            ],
            "view/title": [
                {
                    "command": "parallels-desktop.tree-view.refresh-vms",
                    "when": "view == parallels-desktop-machines && parallels-desktop:initialized",
                    "group": "navigation@1"
                },
                {
                    "command": "parallels-desktop.tree-view.add-vm",
                    "when": "view == parallels-desktop-machines && parallels-desktop:initialized",
                    "group": "navigation@0"
                },
                {
                    "command": "parallels-desktop.core.enable-show-hidden-items",
                    "when": "view == parallels-desktop-machines && parallels-desktop:disableShowHidden && parallels-desktop:initialized",
                    "group": "navigation@2"
                },
                {
                    "command": "parallels-desktop.core.disable-show-hidden-items",
                    "when": "view == parallels-desktop-machines && parallels-desktop:enableShowHidden && parallels-desktop:initialized",
                    "group": "navigation@2"
                },
                {
                    "command": "parallels-desktop.tree-view.add-group",
                    "when": "view == parallels-desktop-machines && parallels-desktop:initialized"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.refresh",
                    "when": "view == parallels-desktop-machines-vagrant && parallels-desktop:initialized",
                    "group": "navigation@0"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.refresh",
                    "when": "view == parallels-desktop-vagrant && parallels-desktop:initialized",
                    "group": "navigation@1"
                },
                {
                    "command": "parallels-desktop.vagrant.search-and-download",
                    "when": "view == parallels-desktop-vagrant && parallels-desktop:vagrant == true",
                    "group": "navigation@2"
                }
            ],
            "view/item/context": [
                {
                    "command": "parallels-desktop.tree-view.take-vm-snapshot",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.(?!macosx)[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\..*/",
                    "group": "inline@19"
                },
                {
                    "command": "parallels-desktop.tree-view.start-vm",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(stopped)/",
                    "group": "inline@0"
                },
                {
                    "command": "parallels-desktop.tree-view.start-headless-vm",
                    "when": "view == parallels-desktop-machines && parallels-desktop:headless-by-default === false && viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(stopped)/"
                },
                {
                    "command": "parallels-desktop.tree-view.start-window-vm",
                    "when": "view == parallels-desktop-machines && parallels-desktop:headless-by-default ===true&& viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(stopped)/"
                },
                {
                    "command": "parallels-desktop.tree-view.enable-rosetta-linux",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.(?!win-11)(?!win-11)[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(rosetta_off)\\.(stopped)/"
                },
                {
                    "command": "parallels-desktop.tree-view.disable-rosetta-linux",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.(?!win-11)(?!win-11)[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(rosetta_on)\\.(stopped)/"
                },
                {
                    "command": "parallels-desktop.tree-view.enter-vm",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(running)/",
                    "group": "inline@21"
                },
                {
                    "command": "parallels-desktop.tree-view.remove-vm",
                    "when": "view == parallels-desktop-machines && (viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(stopped)/ || viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(invalid)/)",
                    "group": "inline@22"
                },
                {
                    "command": "parallels-desktop.tree-view.stop-vm",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(running)/",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.resume-vm",
                    "when": "view == parallels-desktop-machines && (viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(paused)/ || viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(suspended)/)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.pause-vm",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(running)/",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.suspend-vm",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(running)/",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.rename-vm",
                    "when": "view == parallels-desktop-machines && viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.[a-z0-9_-]{0,100}\\.(stopped)/",
                    "group": "inline@23"
                },
                {
                    "command": "parallels-desktop.tree-view.delete-vm-snapshot",
                    "when": "view == parallels-desktop-machines && (viewItem == snapshot.other || viewItem == snapshot.current)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.restore-vm-snapshot",
                    "when": "view == parallels-desktop-machines && viewItem == snapshot.other",
                    "group": "inline@1"
                },
                {
                    "command": "parallels-desktop.tree-view.remove-group",
                    "when": "view == parallels-desktop-machines && viewItem =~ /group\\..*/",
                    "group": "inline@24"
                },
                {
                    "command": "parallels-desktop.tree-view.show-item",
                    "when": "view == parallels-desktop-machines && (viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.hidden\\.[a-z0-9_-]{0,100}\\..*/ || viewItem =~ /group\\.hidden\\..*/)",
                    "group": "inline@25"
                },
                {
                    "command": "parallels-desktop.tree-view.hide-item",
                    "when": "view == parallels-desktop-machines && (viewItem =~ /vm\\.[a-z0-9_-]{0,100}\\.visible\\.[a-z0-9_-]{0,100}\\..*/ || viewItem =~ /group\\.visible\\..*/)",
                    "group": "inline@25"
                },
                {
                    "command": "parallels-desktop.tree-view.rename-group",
                    "when": "view == parallels-desktop-machines && viewItem =~ /group\\..*/",
                    "group": "inline@26"
                },
                {
                    "command": "parallels-desktop.tree-view.start-group-vms",
                    "when": "view == parallels-desktop-machines && (viewItem =~ /group\\.[a-z0-9_-]{0,100}\\.stopped/ || viewItem =~ /group\\.[a-z0-9_-]{0,100}\\.mixed/)",
                    "group": "inline@1"
                },
                {
                    "command": "parallels-desktop.tree-view.stop-group-vms",
                    "when": "view == parallels-desktop-machines && viewItem =~ /group\\.[a-z0-9_-]{0,100}\\.running/",
                    "group": "inline@2"
                },
                {
                    "command": "parallels-desktop.tree-view.resume-group-vms",
                    "when": "view == parallels-desktop-machines && (viewItem =~ /group\\.[a-z0-9_-]{0,100}\\.paused/ || viewItem =~ /group\\.[a-z0-9_-]{0,100}\\.suspended/)",
                    "group": "inline@3"
                },
                {
                    "command": "parallels-desktop.tree-view.pause-group-vms",
                    "when": "view == parallels-desktop-machines && viewItem =~ /group\\.[a-z0-9_-]{0,100}\\.running/",
                    "group": "inline@4"
                },
                {
                    "command": "parallels-desktop.tree-view.suspend-group-vms",
                    "when": "view == parallels-desktop-machines && viewItem =~ /group\\.[a-z0-9_-]{0,100}\\.running/",
                    "group": "inline@5"
                },
                {
                    "command": "parallels-desktop.tree-view.take-group-snapshot",
                    "when": "view == parallels-desktop-machines && viewItem =~ /group\\..*/",
                    "group": "inline@6"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.init",
                    "when": "view == parallels-desktop-vagrant && viewItem == vagrant.box",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.delete",
                    "when": "view == parallels-desktop-vagrant && viewItem == vagrant.box",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.docker.remove-container",
                    "when": "view == parallels-desktop-machines && viewItem =~ /docker\\.container\\.(exited|created)/",
                    "group": "inline@30"
                },
                {
                    "command": "parallels-desktop.docker.start-container",
                    "when": "view == parallels-desktop-machines && viewItem =~ /docker\\.container\\.exited/",
                    "group": "inline@1"
                },
                {
                    "command": "parallels-desktop.docker.stop-container",
                    "when": "view == parallels-desktop-machines && viewItem =~ /docker\\.container\\.(running|paused)/",
                    "group": "inline@1"
                },
                {
                    "command": "parallels-desktop.docker.pause-container",
                    "when": "view == parallels-desktop-machines && viewItem =~ /docker\\.container\\.running/",
                    "group": "inline@2"
                },
                {
                    "command": "parallels-desktop.docker.resume-container",
                    "when": "view == parallels-desktop-machines && viewItem =~ /docker\\.container\\.paused/",
                    "group": "inline@2"
                },
                {
                    "command": "parallels-desktop.docker.restart-container",
                    "when": "view == parallels-desktop-machines && viewItem =~ /docker\\.container\\.running/",
                    "group": "inline@3"
                },
                {
                    "command": "parallels-desktop.docker.get-container-logs",
                    "when": "view == parallels-desktop-machines && viewItem =~ /docker\\.container\\.(?!root).*/",
                    "group": "inline@4"
                },
                {
                    "command": "parallels-desktop.docker.enter-container",
                    "when": "view == parallels-desktop-machines && viewItem =~ /docker\\.container\\.running/",
                    "group": "inline@5"
                },
                {
                    "command": "parallels-desktop.docker.run-container",
                    "when": "view == parallels-desktop-machines && parallels-desktop:docker-container-items && viewItem =~ /docker\\.container\\.root/",
                    "group": "inline@0"
                },
                {
                    "command": "parallels-desktop.docker.remove-image",
                    "when": "view == parallels-desktop-machines && parallels-desktop:docker-container-items && viewItem =~ /docker\\.image\\.(?!root).*/",
                    "group": "inline@0"
                }
            ]
        },
        "views": {
            "parallels-desktop": [
                {
                    "id": "parallels-desktop-machines",
                    "name": "Virtual Machines",
                    "icon": "media/parallels_logo.svg",
                    "contextualTitle": "Parallels Desktop Virtual Machines"
                },
                {
                    "when": "parallels-desktop:vagrant == true",
                    "id": "parallels-desktop-vagrant",
                    "name": "Vagrant Boxes",
                    "icon": "media/parallels_logo.svg",
                    "contextualTitle": "Vagrant Boxes",
                    "type": "tree"
                },
                {
                    "id": "parallels-desktop-help",
                    "name": "Helpful Links",
                    "icon": "media/parallels_logo.svg",
                    "contextualTitle": "Helpful",
                    "visibility": "collapsed"
                }
            ]
        },
        "viewsWelcome": [
            {
                "view": "parallels-desktop-machines",
                "when": "parallels-desktop:initialized == true && parallels-desktop:hasVirtualMachines == false",
                "contents": "Welcome to Parallels Desktop\n Please add a virtual machine to start using the extension.\n[Create Virtual Machine](command:parallels-desktop.tree-view.add-vm)"
            },
            {
                "view": "parallels-desktop-vagrant",
                "when": "parallels-desktop:hasVagrantBoxes === undefined",
                "contents": "Loading Vagrant Boxes..."
            },
            {
                "view": "parallels-desktop-vagrant",
                "when": "parallels-desktop:initialized == true && parallels-desktop:vagrant == true && parallels-desktop:hasVagrantBoxes == false",
                "contents": "Welcome to Vagrant Boxes Management\n Please add a Vagrant box to start using the extension."
            },
            {
                "view": "parallels-desktop-help",
                "contents": "Welcome to Parallels Desktop\n Please use the follow links to get help\n[Discord Server](https://discord.gg/etqdafGvjK)\n[Report a Bug](https://github.com/Parallels/parallels-vscode-extension/issues/new?assignees=&labels=bug&projects=&template=bug_report.md&title=)\n[Request a Feature](https://github.com/Parallels/parallels-vscode-extension/issues/new?assignees=&labels=enhancement&projects=&template=feature_request.md&title=)"
            }
        ]
    },
    "scripts": {
        "clean": "rimraf ./dist ./out",
        "vscode:prepublish": "npm run build",
        "compile": "webpack && gulp copy-images",
        "watch": "webpack --watch",
        "build": "webpack --mode production && gulp copy-images",
        "package": "npm run clean && vsce package",
        "open-in-browser": "vscode-test-web --extensionDevelopmentPath=src .",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "lint": "eslint src --ext ts",
        "lint-fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"**/*.ts\"",
        "format-check": "prettier --check \"**/*.ts\"",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/uuid": "^9.0.1",
        "@types/vscode": "^1.77.0",
        "@types/axios": "^0.14.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.0",
        "@vscode/test-web": "*",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "glob": "^8.1.0",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.3.0",
        "mocha": "^10.2.0",
        "prettier": "^2.8.3",
        "rimraf": "^3.0.1",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.5",
        "webpack": "^5.76.3",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "@vscode/vsce": "^2.19.0",
        "axios": "^1.4.0",
        "tailwindcss": "^3.3.2",
        "uuid": "^9.0.0",
        "clipboardy": "^3.0.0"
    }
