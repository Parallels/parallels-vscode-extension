{
    "name": "parallels-desktop",
    "displayName": "Parallels Desktop",
    "publisher": "ParallelsDesktop",
    "description": "Manage Parallels Desktop virtual machines, their snapshots and containers",
    "repository": {
        "type": "git",
        "url": "https://github.com/Parallels/parallels-vscode-extension"
    },
    "icon": "img/logo/parallels_logo.png",
    "version": "1.0.13",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Testing"
    ],
    "badges": [
        {
            "url": "https://github.com/Parallels/parallels-vscode-extension/actions/workflows/build.yml/badge.svg?branch=main",
            "href": "https://github.com/Parallels/parallels-vscode-extension/actions/workflows/build.yml",
            "description": "Build Status"
        }
    ],
    "preview": false,
    "keywords": [
        "parallels",
        "virtualization",
        "vagrant",
        "packer",
        "prlctl",
        "docker"
    ],
    "homepage": "https://parallels.com",
    "activationEvents": [
        "onView:nodeDependencies"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "configuration": {
            "title": "Parallels Desktop",
            "properties": {
                "parallels-desktop.output_path": {
                    "type": "string",
                    "default": "/Users/cjlapao/parallels_extension",
                    "description": "Output path for Packer images"
                },
                "parallels-desktop.prlctl.path": {
                    "type": "string",
                    "default": "/usr/local/bin/prlctl",
                    "description": "Path to prlctl executable"
                },
                "parallels-desktop.extension.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path for the Parallels Desktop extension"
                },
                "parallels-desktop.extension.download.path": {
                    "type": "string",
                    "default": "",
                    "description": "Download cache folder for the VMs ISO's"
                },
                "parallels-desktop.hashicorp.vagrant.boxes.path": {
                    "type": "string",
                    "default": "",
                    "description": "Vagrant box files folder for new VMs"
                },
                "parallels-desktop.hashicorp.vagrant.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path to vagrant executable"
                },
                "parallels-desktop.hashicorp.vagrant.parallels.plugin": {
                    "type": "boolean",
                    "default": false,
                    "readOnly": true,
                    "description": "Is Parallels plugin installed for Vagrant"
                },
                "parallels-desktop.hashicorp.packer.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path to packer executable"
                },
                "parallels-desktop.git.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path to git executable"
                },
                "parallels-desktop.brew.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path to brew executable"
                },
                "parallels-desktop.extension.refresh.auto": {
                    "type": "boolean",
                    "default": true,
                    "description": "Set Auto Refresh for Parallels Desktop Tree View"
                },
                "parallels-desktop.extension.refresh.interval": {
                    "type": "number",
                    "default": 60000,
                    "description": "Auto Refresh interval in milliseconds"
                },
                "parallels-desktop.tree.show-hidden-items": {
                    "type": "boolean",
                    "default": false,
                    "description": "Shows Hidden items in the tree"
                },
                "parallels-desktop.extension.order-items-alphabetically": {
                    "type": "boolean",
                    "default": true,
                    "description": "Order Groups and VMs alphabetically"
                },
                "parallels-desktop.extension.show-flat-snapshot-tree": {
                    "type": "boolean",
                    "default": false,
                    "description": "Show snapshots as a flat list"
                },
                "parallels-desktop.extension.start-machines-headless-by-default": {
                    "type": "boolean",
                    "default": false,
                    "description": "Start Machines in headless mode by default"
                }
            }
        },
        "commands": [
            {
                "command": "parallels-desktop.tree-view.add-vm",
                "title": "Parallels Desktop: Add VM",
                "shortTitle": "Add VM",
